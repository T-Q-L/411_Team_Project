extends layout
block head
    script(src="https://code.jquery.com/jquery-3.3.1.min.js"
        integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
        crossorigin="anonymous")
    h1= title
    p Welcome to #{title}
    input#input(type="text", placeholder="Enter 5 digit US zipcode", name="location_input")
    p Rental Prices within the given zipcode:
    ul#rental_prices
    script(src="javascripts/rental_price_query.js")
